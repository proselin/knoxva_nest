{
  "name": "knoxva",
  "version": "0.0.1",
  "description": "Nestjs service for tdh project",
  "author": "Hưng Nguyễn Quốc",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "nest:build": "nest build && npm run copy:assets",
    "nest:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "nest:start": "nest start -b swc",
    "nest:watch": "nest start -b swc --watch",
    "nest:debug": "npm run nest:start -- --debug",
    "build": "tsc -p tsconfig.build.json && npm run copy:assets",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": " npm run copy:assets && nest start --watch",
    "start:debug": " npm run copy:assets && nest start --debug --watch",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "debug": "nest start --debug",
    "copy:assets": "cpx \"src/assets/**/*\" \"dist/assets\" ",
    "pm2:run": "pm2 start",
    "pm2:start": "npm run nest:build && npm run pm2:run"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.2.7",
    "axios": "^1.5.0",
    "bull": "^4.11.3",
    "canvas": "^2.11.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "konva": "^9.2.0",
    "nest-winston": "^1.9.4",
    "nestjs-minio": "^2.5.1",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.92",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "cpx": "^1.5.0",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
